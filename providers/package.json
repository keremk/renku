{
  "name": "@gorenku/providers",
  "version": "0.1.4",
  "description": "AI provider integrations for Renku",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "test": "vitest --config vitest.config.ts",
    "test:typecheck": "tsc --project tsconfig.vitest.json --noEmit",
    "check:all": "pnpm type-check && pnpm lint && pnpm test:typecheck",
    "test:ui": "vitest --config vitest.config.ts --ui",
    "test:run": "vitest run --config vitest.config.ts",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "test:e2e": "vitest --config vitest.e2e.config.ts"
  },
  "keywords": [
    "ai",
    "providers",
    "openai",
    "replicate",
    "elevenlabs"
  ],
  "author": "",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@ai-sdk/openai": "^3.0.1",
    "@fal-ai/client": "^1.7.2",
    "@gorenku/compositions": "workspace:*",
    "@gorenku/core": "workspace:*",
    "ai": "^6.0.3",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "mediabunny": "^1.24.5",
    "replicate": "^1.2.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@types/node": "^20.16.11",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-prettier": "^10.1.8",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
