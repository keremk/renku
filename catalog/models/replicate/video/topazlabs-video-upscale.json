{
  "type": "object",
  "title": "Input",
  "required": [
    "video"
  ],
  "properties": {
    "video": {
      "type": "string",
      "title": "Video",
      "format": "uri",
      "x-order": 0,
      "description": "Video file to upscale"
    },
    "target_fps": {
      "type": "integer",
      "title": "Target Fps",
      "default": 60,
      "maximum": 120,
      "minimum": 15,
      "x-order": 2,
      "description": "Target FPS (choose from 15fps to 120fps)"
    },
    "target_resolution": {
      "allOf": [
        {
          "$ref": "#/components/schemas/target_resolution"
        }
      ],
      "default": "1080p",
      "x-order": 1,
      "description": "Target resolution"
    }
  }
}
