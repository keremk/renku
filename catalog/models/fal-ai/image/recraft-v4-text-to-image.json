{
  "input_schema": {
    "x-fal-order-properties": [
      "prompt",
      "image_size",
      "colors",
      "background_color",
      "enable_safety_checker"
    ],
    "type": "object",
    "properties": {
      "prompt": {
        "examples": [
          "Tilt-shift miniature effect on a real Portuguese fishing village at golden hour, colorful boats in the harbor appearing toy-like, selective focus band across the middle, saturated primary colors of blue red and yellow boats against white buildings, 90mm tilt-shift lens, the familiar made fantastical, Wes Anderson color sensibility"
        ],
        "maxLength": 10000,
        "type": "string",
        "minLength": 1,
        "title": "Prompt"
      },
      "image_size": {
        "examples": [
          "landscape_16_9"
        ],
        "title": "Image Size",
        "anyOf": [
          {
            "$ref": "#/ImageSize"
          },
          {
            "enum": [
              "square_hd",
              "square",
              "portrait_4_3",
              "portrait_16_9",
              "landscape_4_3",
              "landscape_16_9"
            ],
            "type": "string"
          }
        ],
        "default": "square_hd"
      },
      "enable_safety_checker": {
        "description": "If set to true, the safety checker will be enabled.",
        "type": "boolean",
        "title": "Enable Safety Checker",
        "default": true
      },
      "colors": {
        "description": "An array of preferable colors",
        "type": "array",
        "title": "Colors",
        "items": {
          "$ref": "#/RGBColor"
        },
        "default": []
      },
      "background_color": {
        "description": "The preferable background color of the generated images.",
        "title": "Background Color",
        "allOf": [
          {
            "$ref": "#/RGBColor"
          }
        ]
      }
    },
    "title": "RecraftV4TextToImageInput",
    "required": [
      "prompt"
    ]
  },
  "output_schema": {
    "x-fal-order-properties": [
      "images"
    ],
    "type": "object",
    "properties": {
      "images": {
        "examples": [
          [
            {
              "file_size": 272628,
              "file_name": "image.webp",
              "content_type": "image/webp",
              "url": "https://storage.googleapis.com/falserverless/example_outputs/recraft-v4/standard-output.webp"
            }
          ]
        ],
        "title": "Images",
        "type": "array",
        "items": {
          "$ref": "#/File"
        }
      }
    },
    "title": "RecraftV4TextToImageOutput",
    "required": [
      "images"
    ]
  },
  "ImageSize": {
    "x-fal-order-properties": [
      "width",
      "height"
    ],
    "type": "object",
    "properties": {
      "height": {
        "maximum": 14142,
        "type": "integer",
        "description": "The height of the generated image.",
        "title": "Height",
        "exclusiveMinimum": 0,
        "default": 512
      },
      "width": {
        "maximum": 14142,
        "type": "integer",
        "description": "The width of the generated image.",
        "title": "Width",
        "exclusiveMinimum": 0,
        "default": 512
      }
    },
    "title": "ImageSize"
  },
  "RGBColor": {
    "x-fal-order-properties": [
      "r",
      "g",
      "b"
    ],
    "type": "object",
    "properties": {
      "r": {
        "minimum": 0,
        "maximum": 255,
        "type": "integer",
        "description": "Red color value",
        "title": "R",
        "default": 0
      },
      "b": {
        "minimum": 0,
        "maximum": 255,
        "type": "integer",
        "description": "Blue color value",
        "title": "B",
        "default": 0
      },
      "g": {
        "minimum": 0,
        "maximum": 255,
        "type": "integer",
        "description": "Green color value",
        "title": "G",
        "default": 0
      }
    },
    "title": "RGBColor"
  },
  "File": {
    "x-fal-order-properties": [
      "url",
      "content_type",
      "file_name",
      "file_size",
      "file_data"
    ],
    "type": "object",
    "properties": {
      "file_size": {
        "examples": [
          4404019
        ],
        "description": "The size of the file in bytes.",
        "type": "integer",
        "title": "File Size"
      },
      "file_name": {
        "examples": [
          "z9RV14K95DvU.png"
        ],
        "description": "The name of the file. It will be auto-generated if not provided.",
        "type": "string",
        "title": "File Name"
      },
      "content_type": {
        "examples": [
          "image/png"
        ],
        "description": "The mime type of the file.",
        "type": "string",
        "title": "Content Type"
      },
      "url": {
        "description": "The URL where the file can be downloaded from.",
        "type": "string",
        "title": "Url",
        "format": "uri"
      },
      "file_data": {
        "format": "binary",
        "description": "File data",
        "type": "string",
        "title": "File Data"
      }
    },
    "title": "File",
    "required": [
      "url"
    ]
  }
}
