{
  "input_schema": {
    "x-fal-order-properties": [
      "video_url",
      "upscale_factor",
      "target_fps",
      "focus",
      "sharpen",
      "H264_output"
    ],
    "type": "object",
    "properties": {
      "H264_output": {
        "title": "H264 Output",
        "type": "boolean",
        "description": "Whether to use H264 codec for output video. Default is H265.",
        "default": false
      },
      "video_url": {
        "examples": [
          "https://v3.fal.media/files/kangaroo/y5-1YTGpun17eSeggZMzX_video-1733468228.mp4"
        ],
        "title": "Video Url",
        "type": "string",
        "description": "URL of the video to upscale",
        "format": "uri"
      },
      "upscale_factor": {
        "minimum": 1,
        "maximum": 4,
        "type": "number",
        "description": "Factor to upscale the video by (e.g. 2.0 doubles width and height)",
        "title": "Upscale Factor",
        "default": 2
      },
      "target_fps": {
        "minimum": 16,
        "maximum": 60,
        "type": "integer",
        "description": "Target FPS for frame interpolation. If set, frame interpolation will be enabled.",
        "title": "Target Fps"
      },
      "focus": {
        "minimum": 0,
        "maximum": 1,
        "type": "number",
        "description": "Focus/sharpening level (0.0-1.0, default 0.2)",
        "title": "Focus",
        "default": 0.2
      },
      "sharpen": {
        "minimum": 0,
        "maximum": 1,
        "type": "number",
        "description": "Sharpening level (0.0-1.0, default 0.6)",
        "title": "Sharpen",
        "default": 0.6
      }
    },
    "title": "VideoUpscaleRequest",
    "required": [
      "video_url"
    ]
  },
  "output_schema": {
    "x-fal-order-properties": [
      "video"
    ],
    "type": "object",
    "properties": {
      "video": {
        "examples": [
          {
            "url": "https://v3.fal.media/files/penguin/ztj_LB4gQlW6HIfVs8zX4_upscaled.mp4"
          }
        ],
        "title": "Video",
        "description": "The upscaled video file",
        "allOf": [
          {
            "$ref": "#/File"
          }
        ]
      }
    },
    "title": "VideoUpscaleOutput",
    "required": [
      "video"
    ]
  },
  "File": {
    "x-fal-order-properties": [
      "url",
      "content_type",
      "file_name",
      "file_size",
      "file_data"
    ],
    "type": "object",
    "properties": {
      "file_size": {
        "examples": [
          4404019
        ],
        "title": "File Size",
        "type": "integer",
        "description": "The size of the file in bytes."
      },
      "file_name": {
        "examples": [
          "z9RV14K95DvU.png"
        ],
        "title": "File Name",
        "type": "string",
        "description": "The name of the file. It will be auto-generated if not provided."
      },
      "content_type": {
        "examples": [
          "image/png"
        ],
        "title": "Content Type",
        "type": "string",
        "description": "The mime type of the file."
      },
      "url": {
        "title": "Url",
        "type": "string",
        "description": "The URL where the file can be downloaded from.",
        "format": "uri"
      },
      "file_data": {
        "format": "binary",
        "title": "File Data",
        "type": "string",
        "description": "File data"
      }
    },
    "title": "File",
    "required": [
      "url"
    ]
  }
}
