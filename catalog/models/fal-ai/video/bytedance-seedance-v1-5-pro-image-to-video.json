{
  "input_schema": {
    "title": "SeedanceProv15ImageToVideoInput",
    "type": "object",
    "properties": {
      "prompt": {
        "examples": [
          "A man is crying and he says \"I shouldn't have done it. I regret everything\""
        ],
        "description": "The text prompt used to generate the video",
        "type": "string",
        "title": "Prompt"
      },
      "aspect_ratio": {
        "enum": [
          "21:9",
          "16:9",
          "4:3",
          "1:1",
          "3:4",
          "9:16"
        ],
        "title": "Aspect Ratio",
        "type": "string",
        "description": "The aspect ratio of the generated video",
        "default": "16:9"
      },
      "resolution": {
        "enum": [
          "480p",
          "720p"
        ],
        "title": "Resolution",
        "type": "string",
        "description": "Video resolution - 480p for faster generation, 720p for balance",
        "default": "720p"
      },
      "generate_audio": {
        "title": "Generate Audio",
        "type": "boolean",
        "description": "Whether to generate audio for the video",
        "default": true
      },
      "duration": {
        "enum": [
          "4",
          "5",
          "6",
          "7",
          "8",
          "9",
          "10",
          "11",
          "12"
        ],
        "title": "Duration",
        "type": "string",
        "description": "Duration of the video in seconds",
        "default": "5"
      },
      "image_url": {
        "examples": [
          "https://v3b.fal.media/files/b/0a8773cd/REzCWn1BKUVuMFTxR-R3W_image_317.png"
        ],
        "description": "The URL of the image used to generate video",
        "type": "string",
        "title": "Image Url",
        "format": "uri"
      },
      "seed": {
        "description": "Random seed to control video generation. Use -1 for random.",
        "type": "integer",
        "title": "Seed"
      },
      "end_image_url": {
        "description": "The URL of the image the video ends with. Defaults to None.",
        "type": "string",
        "title": "End Image Url",
        "format": "uri"
      },
      "camera_fixed": {
        "title": "Camera Fixed",
        "type": "boolean",
        "description": "Whether to fix the camera position",
        "default": false
      }
    },
    "x-fal-order-properties": [
      "prompt",
      "aspect_ratio",
      "resolution",
      "duration",
      "camera_fixed",
      "seed",
      "generate_audio",
      "image_url",
      "end_image_url"
    ],
    "required": [
      "prompt",
      "image_url"
    ]
  },
  "output_schema": {
    "title": "SeedanceProv15I2VVideoOutput",
    "type": "object",
    "properties": {
      "seed": {
        "examples": [
          42
        ],
        "description": "Seed used for generation",
        "type": "integer",
        "title": "Seed"
      },
      "video": {
        "examples": [
          {
            "url": "https://v3b.fal.media/files/b/0a8773d3/l2fk-fIO_PQFPzbvHkQX1_video.mp4"
          }
        ],
        "description": "Generated video file",
        "title": "Video",
        "allOf": [
          {
            "$ref": "#/File"
          }
        ]
      }
    },
    "x-fal-order-properties": [
      "video",
      "seed"
    ],
    "required": [
      "video",
      "seed"
    ]
  },
  "File": {
    "title": "File",
    "type": "object",
    "properties": {
      "file_size": {
        "examples": [
          4404019
        ],
        "description": "The size of the file in bytes.",
        "type": "integer",
        "title": "File Size"
      },
      "file_name": {
        "examples": [
          "z9RV14K95DvU.png"
        ],
        "description": "The name of the file. It will be auto-generated if not provided.",
        "type": "string",
        "title": "File Name"
      },
      "content_type": {
        "examples": [
          "image/png"
        ],
        "description": "The mime type of the file.",
        "type": "string",
        "title": "Content Type"
      },
      "url": {
        "description": "The URL where the file can be downloaded from.",
        "type": "string",
        "title": "Url",
        "format": "uri"
      },
      "file_data": {
        "format": "binary",
        "description": "File data",
        "type": "string",
        "title": "File Data"
      }
    },
    "x-fal-order-properties": [
      "url",
      "content_type",
      "file_name",
      "file_size",
      "file_data"
    ],
    "required": [
      "url"
    ]
  }
}
