# Renku Environment Variables
# Copy to .env and fill in your API keys

# AI Provider API Keys
OPENAI_API_KEY=
REPLICATE_API_TOKEN=
FAL_KEY=
WAVESPEED_API_KEY=
ELEVENLABS_API_KEY=

# Cloud Storage (S3-compatible)
S3_REGION=auto
S3_ENDPOINT=
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_BUCKET=

# Cloudflare
CLOUDFLARE_TOKEN=

# Optional Configuration
# RENKU_VIEWER_ROOT=~/renku-data
# RENKU_CLI_CONFIG=~/.config/renku/cli-config.json

# Test Flags (set to '1' to enable)
# RUN_OPENAI_TEST=
# RUN_FAL_TEST=
# RUN_ELEVENLABS_TEST=
# RUN_REPLICATE_TEST=
